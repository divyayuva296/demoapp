<%= link_to 'new post' , new_post_path %><br>

<%= link_to 'logout', destroy_user_session_path,method: :delete %><br>
<%= link_to 'search post', posts_search_path %>


<% @posts.each do |post| %>
	<p>
		<%= post.title %><br>
		<%= post.content %><br>
		<div>
			<%= link_to 'show', post_path(post) %>
			<%= link_to 'edit', edit_post_path(post) %>
			<%= link_to 'destroy',post_path(post), method: :delete %>
			
		</div>
	<p>
	
<% end %>

<h1>Search form</h1>
<%= form_with(url: "/posts", method: "get") do %>
	<%= label_tag(:search, "Search for:")%>
	<%= text_field_tag(:search, params[:search]) %>
	<%= submit_tag("Search") %>
<% end %>

